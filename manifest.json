{
  "author": "Raymond Hill",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "match_about_blank": true,
      "js": ["contentscript.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "description": "A companion extension to uBlock Origin: this gives uBlock Origin the ability to see and filter WebSocket connection attempts.",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "36.0",
  "name": "uBlock Origin WebSocket",
  "short_name": "uBO-WebSocket",
  "version": "1.1"
}
